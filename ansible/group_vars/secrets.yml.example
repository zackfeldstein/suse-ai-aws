---
# SUSE App Collection Credentials for Rancher Prime
# Copy this file to secrets.yml and fill in your actual credentials
# 
# Get these credentials from:
# https://www.suse.com/support/
# 
# IMPORTANT: Add secrets.yml to .gitignore to prevent committing credentials!

app_collection_username: "your-suse-username"
app_collection_password: "your-suse-password"

# Example:
# app_collection_username: "john.doe@company.com"
# app_collection_password: "your-secure-password"

